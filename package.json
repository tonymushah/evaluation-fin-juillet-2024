{
	"name": "evaluation-fin-juillet-2024",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"proto:commons": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path protos protos/commons.proto",
		"proto:admin": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path protos protos/admin.proto",
		"proto:client": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path protos protos/client.proto",
		"proto:all": "pnpm proto:commons && pnpm proto:admin && pnpm proto:client",
		"back:admin": "cargo run --bin admin",
		"back:client": "cargo run --bin client",
		"back:seed": "cargo run --bin seed"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.28.1",
		"@protobuf-ts/plugin": "^2.9.4",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^9.6.0",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.14.12",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"flowbite": "^2.4.1",
		"flowbite-svelte": "^0.46.15",
		"flowbite-svelte-icons": "^1.6.1",
		"globals": "^15.0.0",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.4",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwind-merge": "^2.4.0",
		"tailwindcss": "^3.4.4",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"vite-plugin-kit-routes": "^0.6.3",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.19",
		"@fontsource/fira-mono": "^5.0.13",
		"@fontsource/freckle-face": "^5.0.20",
		"@fontsource/poppins": "^5.0.14",
		"@protobuf-ts/grpcweb-transport": "^2.9.4",
		"@protobuf-ts/runtime": "^2.9.4",
		"@protobuf-ts/runtime-rpc": "^2.9.4",
		"dayjs": "^1.11.12",
		"lodash-es": "^4.17.21",
		"uuid": "^10.0.0"
	}
}
