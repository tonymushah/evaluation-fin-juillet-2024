{
	"name": "evaluation-fin-juillet-2024",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"proto:commons": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path protos protos/commons.proto",
		"proto:admin": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path protos protos/admin.proto",
		"proto:client": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path protos protos/client.proto",
		"proto:all": "pnpm proto:commons && pnpm proto:admin && pnpm proto:client",
		"back:admin": "cargo run --bin admin",
		"back:client": "cargo run --bin client",
		"back:admin-web": "cargo run --bin admin-web",
		"back:client-web": "cargo run --bin client-web",
		"back:seed": "cargo run --bin seed",
		"back:reset-no-seed": "cargo run --bin reset-no-seed",
		"back:reset": "pnpm back:reset-no-seed && pnpm back:seed"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@playwright/test": "^1.46.1",
		"@protobuf-ts/plugin": "^2.9.4",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/adapter-node": "^5.2.2",
		"@sveltejs/kit": "^2.5.25",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.16.2",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"flowbite": "^2.5.1",
		"flowbite-svelte": "^0.46.15",
		"flowbite-svelte-icons": "^1.6.1",
		"globals": "^15.9.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"tailwind-merge": "^2.5.2",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.3.0",
		"vite": "^5.4.2",
		"vite-plugin-kit-routes": "^0.6.10",
		"vitest": "^2.0.5"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.20",
		"@fontsource/fira-mono": "^5.0.14",
		"@fontsource/freckle-face": "^5.0.21",
		"@fontsource/poppins": "^5.0.15",
		"@grpc/grpc-js": "^1.11.1",
		"@protobuf-ts/grpc-transport": "^2.9.4",
		"@protobuf-ts/grpcweb-transport": "^2.9.4",
		"@protobuf-ts/runtime": "^2.9.4",
		"@protobuf-ts/runtime-rpc": "^2.9.4",
		"dayjs": "^1.11.13",
		"lodash-es": "^4.17.21",
		"socket.io": "^4.7.5",
		"socket.io-client": "^4.7.5",
		"uuid": "^10.0.0"
	}
}
